<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record model="ir.ui.view" id="product_pricelist_view">
    <field name="name">Membership field to product pricelist</field>
    <field name="model">product.pricelist</field>
    <field name="inherit_id" ref="product.product_pricelist_view" />
    <field name="arch" type="xml">
        <group name="pricelist_discounts" position="after">
            <group name="settings" string="Settings">
                <field name="membership_pricelist" />
            </group>
        </group>
    </field>
</record>

</odoo>
