<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="assets_frontend"
        inherit_id="web.assets_frontend"
        name="only_one_membership_in_cart assets"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/only_one_membership_in_cart/static/src/js/check_product_in_cart.js"
            />
        </xpath>
    </template>

    <template
        id="website_assets_frontend"
        inherit_id="website.assets_frontend"
        name="only_one_membership_in_cart Assets Frontend"
    >
        <xpath expr="link[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/only_one_membership_in_cart/static/src/scss/main.scss"
            />
        </xpath>
    </template>
</odoo>
